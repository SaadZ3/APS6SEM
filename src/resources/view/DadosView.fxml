<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.DadosController">
    <children>
        <VBox layoutX="14.0" layoutY="14.0" spacing="10.0" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
            <children>
                <Label fx:id="lblTitulo" text="Dados Estratégicos (Nível X)">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <TableView fx:id="tabelaPropriedades" prefHeight="350.0" prefWidth="552.0">
                    <columns>
                        <TableColumn fx:id="colPropriedade" prefWidth="150.0" text="Propriedade" />
                        <TableColumn fx:id="colAgrotoxico" prefWidth="150.0" text="Agrotóxico" />
                        <TableColumn fx:id="colImpacto" prefWidth="190.0" text="Impacto / Risco" />
                        <TableColumn fx:id="colNivel" prefWidth="60.0" text="Nível" />
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>