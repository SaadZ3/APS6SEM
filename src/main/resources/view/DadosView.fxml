<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.DadosController">
    <children>
        <VBox spacing="10.0" maxWidth="800.0">
            <children>
                <Label fx:id="lblTitulo" text="Dados Estratégicos (Nível X)">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <TableView fx:id="tabelaPropriedades" prefHeight="450.0" prefWidth="760.0">
                    <columns>
                        <TableColumn fx:id="colPropriedade" prefWidth="200.0" text="Propriedade" />
                        <TableColumn fx:id="colAgrotoxico" prefWidth="200.0" text="Agrotóxico" />
                        <TableColumn fx:id="colImpacto" prefWidth="300.0" text="Impacto / Risco" />
                        <TableColumn fx:id="colNivel" prefWidth="60.0" text="Nível" />
                    </columns>
                </TableView>
                <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#voltarParaHome" prefWidth="200.0" text="Voltar à Tela Inicial" />
            </children>
        </VBox>
    </children>
    <padding>
        <Insets top="20.0" />
    </padding>
</VBox>